#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main(void)
{
	setbuf(stdout,NULL);
	char texto[] = "hola nona";
	printf("\n el texto es : %s",texto);

	strcpy(texto,"Pedro");

	printf("\n el texto es : %s",texto);
	return EXIT_SUCCESS;
}
