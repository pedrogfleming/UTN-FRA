#include <stdio.h>
#include <stdlib.h>
#define MAX 2

int main(void)
{
	setbuf(stdin,NULL);
	int numeros[MAX];
	swapear(&numeros[]);
	return EXIT_SUCCESS;
}

int swapear(int* arrayPuntero[MAX])
{
	if(arrayPuntero != NULL)
	{
		int arrayLocal [MAX] = arrayPuntero[MAX];
		int aux;
		int i;
		int j;
		aux = arrayLocal[1];
		arrayLocal[2] = arrayLocal[1];
		arrayLocal [1] = aux;

	}

	return 0;
}


